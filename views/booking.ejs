<%- include('partial/header') %>
<%- include('partial/navbar') %>
<div class="row">
    <div class="col-lg-12">
        <h1>Doctor</h1>
    </div>
    <% for (let i = 0; i < doctor.length; i++) { %>
    <div class="col-lg-3">
        <div class="card">
            <img class="card-img-top" src="/uploads/doctor/<%= doctor[i].image %>" alt="">
            <div class="card-body">
                <p><%= doctor[i].name %></p>
                <p><%= doctor[i].speciality %></p>
                <a name="" id="" class="btn btn-primary" href="/doctor_booking/<%= doctor[i]._id %>" role="button">Book Now</a>
            </div>
        </div>
    </div>
    <% } %>
</div>
<%- include('partial/footer') %>